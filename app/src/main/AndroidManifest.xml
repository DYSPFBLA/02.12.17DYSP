<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fbla.mobileapp.app.dysp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/dysp_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="fbla.mobileapp.app.dysp.Add_Object"
            android:label="@string/title_activity_add__object"
            android:theme="@style/AppTheme" />
        <activity
            android:name="fbla.mobileapp.app.dysp.Add_Details"
            android:label="@string/title_activity_add__details"
            android:theme="@style/AppTheme" />
        <activity android:name="fbla.mobileapp.app.dysp.List_Item"
            android:windowSoftInputMode="stateHidden"/>
        <activity
            android:name="fbla.mobileapp.app.dysp.SplashScreenv2"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="fbla.mobileapp.app.dysp.LoginActivity" />
        <activity android:name="fbla.mobileapp.app.dysp.SignIn" />
        <activity android:name="fbla.mobileapp.app.dysp.AccountInformation" />
        <activity
            android:name="fbla.mobileapp.app.dysp.NavigationActivity"
            android:label="@string/title_activity_navigation"
            android:theme="@style/AppTheme" />
        <activity android:name="fbla.mobileapp.app.dysp.ViewItem" />
        <activity android:name="fbla.mobileapp.app.dysp.ViewComments" />
        <activity android:name="fbla.mobileapp.app.dysp.CreditCard" />
        <activity android:name="fbla.mobileapp.app.dysp.ThankYouTo" />
    </application>

</manifest>